<h2>Blocked users</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nickname</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.nickname %></td>
      <td><%= link_to user.name, user_profile_path(user) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
